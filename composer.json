{
    "name": "zicht/yaml-utils",
    "type": "command",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "symfony/console": "^4.2",
        "symfony/finder": "^4.2",
        "symfony/process": "^4.2"
    },
    "description": "an yaml helper library for auto-fixing files to new yaml standard",
    "authors": [
        {
            "name": "Philip Bergman",
            "email": "philip@zicht.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src"
        }
    },
    "license": "MIT",
    "scripts": {
        "post-install-cmd": [
            "git submodule update --init"
        ],
        "post-update-cmd": [
            "git submodule update --init"
        ]
    }
}
